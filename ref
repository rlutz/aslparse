
n = UInt(Rn);
registers = register_list;
increment = U == '1';
wordhigher = P == U;
if n == 15 || BitCount(registers) < 1 then
    UNPREDICTABLE;


BitCount(registers) < 1


if ConditionPassed() then
    EncodingSpecificOperations();
    if PSTATE.EL == EL2 then
        UNDEFINED;
    elsif PSTATE.M IN {M32_User, M32_System} then
        UNPREDICTABLE;
    else
        length = 4 * BitCount(registers);
        address = increment ? R[n] : R[n] - length;
        if wordhigher then
            address = address + 4;
        for i = 0 to 14
            if registers<i> == '1' then
                Rmode[i, M32_User] = MemA[address, 4];
                address = address + 4;


PSTATE.M IN {M32_User, M32_System}

